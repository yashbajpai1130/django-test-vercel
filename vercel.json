{
    "version" : 2,
    "builds": [{
        "src": "vercel_test/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime" : "python3.9"}
    },
    {
        "src" : "build_files.sh",
        "use" : "@vercel/static-build"
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_test/wsgi.py"
        }
    ]
}